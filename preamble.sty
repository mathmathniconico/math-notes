\% utils

\newcommand{\qed}{\square}			\% quod erat demonstrandum
\newcommand{\of}{\mathbin{:}}		\% type of function
\DeclareMathOperator{\mhyphen}{-}	\% math hypen
\newcommand{\cat}[1]{\mathrm{#1}}							\% category name
\newcommand{\ccat}[2]{#1\mhyphen\mathrm{#2}}				\% category name with coefficient
\newcommand{\bmatrix}[1]{\begin{bmatrix} #1 \end{bmatrix}}	\% square bracket matrix

\% parenthesis
\newcommand{\parens}[1]{\left( #1 \right)}					\% parenthesis
\newcommand{\verts}[1]{\left\vert #1 \right\vert}			\% vertical line
\newcommand{\curls}[1]{\left\lbrace #1 \right\rbrace}		\% brace
\newcommand{\bracks}[1]{\left\lbrack #1 \right\rbrack}		\% brack
\newcommand{\angles}[1]{\left\langle #1 \right\rangle}		\% angle
\newcommand{\Verts}[1]{\left\vert\left\vert #1 \right\vert\right\vert}			\% double vertical line
\newcommand{\Bracks}[1]{\left\lbrack\left\lbrack #1 \right\rbrack\right\rbrack}	\% double vertical line, llbrack is not supported in mathjax

